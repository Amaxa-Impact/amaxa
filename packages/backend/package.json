{
  "name": "@amaxa/backend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "license": "MIT",
  "exports": {
    "./convex/*": "./convex/*.ts",
    "./convex/_generated/*": "./convex/_generated/*.js"
  },
  "scripts": {
    "dev": "convex dev",
    "dev:setup": "convex dev --configure --until-success",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "devDependencies": {
    "@amaxa/eslint-config": "workspace:*",
    "@amaxa/prettier-config": "workspace:*",
    "@amaxa/tsconfig": "workspace:*",
    "@convex-dev/migrations": "^0.2.9",
    "convex": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@amaxa/prettier-config"
}
